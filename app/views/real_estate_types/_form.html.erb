<%= form_for(@real_estate_type) do |f| %>
  <% if @real_estate_type.errors.any? %>
    <div id="error_explanation">
      <h2>Nije moguće kreirati tip nekretnine zbog sljedećih grešaka:</h2>

      <ul>
      <% @real_estate_type.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :Tip_nekretnine %><br />
    <%= f.text_field :realEstateType %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
