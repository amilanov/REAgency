<h1>Listing real_estate_details</h1>

<table>
  <tr>
    <th>Size</th>
    <th>Images</th>
    <th>Realestatetype</th>
    <th>Condition</th>
    <th>Note</th>
    <th>Real estate</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @real_estate_details.each do |real_estate_detail| %>
  <tr>
    <td><%= real_estate_detail.size %></td>
    <td><%= real_estate_detail.images %></td>
    <td><%= real_estate_detail.realEstateType %></td>
    <td><%= real_estate_detail.condition %></td>
    <td><%= real_estate_detail.note %></td>
    <td><%= real_estate_detail.real_estate_id %></td>
    <td><%= link_to 'Show', real_estate_detail %></td>
    <td><%= link_to 'Edit', edit_real_estate_detail_path(real_estate_detail) %></td>
    <td><%= link_to 'Destroy', real_estate_detail, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

