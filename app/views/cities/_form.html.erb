<%= form_for(@city) do |f| %>
<% byebug %>
  <% if @city && @city.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@city.errors.count, "error") %> prohibited this city from being saved:</h2>

      <ul>
      <% @city.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :cityName %><br />
    <%= f.text_field :cityName %>
  </div>
  <div class="field">
    <%= f.label :postalCode %><br />
    <%= f.text_field :postalCode %>
  </div>
  <div class="field">
    <%= f.collection_select :country_id, @countries, :id, :countryName, {:include_blank => 'Please Select'}, {class: "btn btn-primary dropdown-toggle"} %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
